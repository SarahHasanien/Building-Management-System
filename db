db name = BMS
CREATE TABLE user ( 
	user_id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, 
	name TEXT NOT NULL,
    type INTEGER NOT NULL,
	email TEXT NOT NULL, 
    flat_number INTEGER, 
	password TEXT NULL );
